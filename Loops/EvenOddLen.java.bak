import java.util.Scanner;
class EvenOddLen
{
	public static void main(String[]args)
	{
		Scanner sc=new Scanner(System.in);
		System.out.print("Enter a number: ");
		int num=sc.nextInt();
		int cnt=0;
		while (num>0)
		{
			cnt++;
			num/=10;
		}
		System.out.println(cnt+" : "+num );
	}
}
