import java.util.Scanner;
class UserName
{
	public static void main(String[]args)
	{
		Scanner sc=new Scanner(System.in);
		System.out.print("Enter your name: ");
		String name=sc.next();
		int len=name.length();
		for (int i=0;i<len ;i++ )
		{
			System.out.print(name.charAt(i));
		}
	}
}