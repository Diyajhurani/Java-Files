class Example2
{
	public static void main(String[] args) 
	{
		byte a1=10;
		byte a2=20;
		int out1=a2-a1;
		System.out.println(out1);

		short b1=200;
		short b2=500;
		int out2=b2-b1;
		System.out.println(out2);

		char c1='a';
		char c2='b';
		int out3=c2-c1;
		System.out.println(out3);

		int d1=2000;
		int d2=9000;
		int out4=d2-d1;
		System.out.println(out4);

		long e1=250l;
		long e2=25000l;
		long out5=e2-e1;
		System.out.println(out5);

		float f1=1.1f;
		float f2=2.2f;
		float out6=f2-f1;
		System.out.println(out6);

		double g1=1.1;
		double g2=2.2;
		double out7=g2-g1;
		System.out.println(out7);

		

	}
